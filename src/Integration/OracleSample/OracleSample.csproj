<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <AssemblyName>OracleSample</AssemblyName>
    <PackageId>OracleSample</PackageId>
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <OutputType>exe</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="App.config" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.1" />
    <PackageReference Include="NServiceBus" Version="6.2.1" />
    <PackageReference Include="Oracle.ManagedDataAccess" Version="12.1.24160719" />
    <ProjectReference Include="$(SolutionDir)Integration\Shared\Shared.csproj" />
    <Reference Include="NServiceBus.Persistence.Sql">
      <SpecificVersion>False</SpecificVersion>
      <HintPath>$(SolutionDir)ForIntegration\NServiceBus.Persistence.Sql.dll</HintPath>
    </Reference>
    <Reference Include="System" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

</Project>
